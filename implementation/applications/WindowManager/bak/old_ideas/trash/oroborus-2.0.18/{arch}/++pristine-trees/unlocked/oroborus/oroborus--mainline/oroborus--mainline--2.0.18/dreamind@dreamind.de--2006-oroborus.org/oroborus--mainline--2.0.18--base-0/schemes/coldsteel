inactive_border_color=#000010
inactive_color_1=#8898aa
inactive_shadow_1=#6e7b8b
inactive_hilight_1=#a2b5cd
inactive_color_2=#a2b5cd
inactive_shadow_2=#6e7b8b
inactive_hilight_2=#b2d2ee